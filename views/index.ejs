<!DOCTYPE html>
<html ng-app="wordManager">
<head>
	<title>Dictionary App</title>
	<link rel="stylesheet" type="text/css" href="styles/mystyle.css">
</head>
<body ng-controller="wineController">
	<div>Please enter the words you want to look up (space delimited)</div>
	<div>
		<input type='text' id='the-word' ng-model='userText' placeholder="Enter words here">
		<button ng-click="getWord();" id='submit-button'>Submit Chat</button>
		
	</div>

	<script type="text/x-handlebars-template" id='users-template'>
	<table>
		<thead>
			<th>First name</th>
			<th>Last name</th>
			<th>Username</th>
			<th>Age</th>
		</thead>
		<tbody>
			{{#each allUsers}}
			<tr>
				<td>{{firstname}}</td>
				<td>{{lastname}}</td>
				<td>{{username}}</td>
				<td>{{age}}</td>
			</tr>
			{{/each}}
		</tbody>


	</table>
</script>

<script src="http://code.jquery.com/jquery-2.1.3.min.js"></script>
<!-- <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.14/angular.min.js"></script> -->
<script src='application.js'></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/3.0.0/handlebars.js"></script>
</body>
</html>